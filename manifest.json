{
  "name": "Misc Enhancer",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Very small enhancements to the misc",

   "background": {
    "scripts": ["background.js"]
  },
   "permissions": [
    "tabs", "http://forum.bodybuilding.com/*"
  ],
    "browser_action": {
    "default_icon": "icon.png"
  },

    "content_scripts" : [{
        "matches" : [ "http://forum.bodybuilding.com/*"],
        "js" : ["js.js", "js2.js"],
        "css" : ["css.css"],
        "run_at": "document_end"  
        }]
}
